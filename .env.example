VITE_SUPABASE_URL=your-project-url
VITE_SUPABASE_ANON_KEY=your-anon-key

# Email Service (for notifications)
VITE_EMAIL_SERVICE_API_KEY=your-email-service-key
VITE_EMAIL_FROM=noreply@milagro.com

# Storage Configuration
VITE_STORAGE_BUCKET_URL=your-storage-bucket-url

# API Configuration
VITE_API_URL=http://localhost:4000/api
VITE_API_TIMEOUT=30000

# Feature Flags
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_ANALYTICS=true

# Admin Configuration
VITE_SUPERADMIN_EMAIL=your-superadmin-email
VITE_DEFAULT_ADMIN_PASSWORD=your-default-password

# Rate Limiting
VITE_RATE_LIMIT_REQUESTS=100
VITE_RATE_LIMIT_WINDOW=60000

# Security
VITE_JWT_EXPIRY=8h
VITE_PASSWORD_MIN_LENGTH=8

# Logging
VITE_LOG_LEVEL=info